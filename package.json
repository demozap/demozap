{
  "name": "demozap",
  "description": "CLI tool for creating demos of React components",
  "version": "0.0.0-development",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "npm run postpack && npm run prepack",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "format-lint": "prettier --config .prettierrc -l --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown .",
    "commit": "git-cz",
    "storybook": "start-storybook -p 9009",
    "storybook-build": "build-storybook",
    "demozap-generate-example-storybook": "./bin/run generate --prefix=_ && npm run format-fix",
    "semantic-release": "semantic-release --branches main"
  },
  "bin": {
    "demozap": "./bin/run"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "demozap",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "chalk": "^4.1.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.1",
    "replace-in-file": "^6.3.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@oclif/dev-cli": "^1.22.2",
    "@storybook/addons": "^6.5.0-beta.0",
    "@storybook/builder-webpack5": "^6.5.0-beta.0",
    "@storybook/manager-webpack5": "^6.5.0-beta.0",
    "@storybook/react": "^6.5.0-beta.0",
    "@storybook/theming": "^6.5.0-beta.0",
    "@types/node": "^17.0.30",
    "@types/eslint": "^8.4.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.14.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-no-array-reduce": "^1.0.55",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.5.11",
    "globby": "^11.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react": "^18.1.0",
    "react-demo-tab": "^1.0.4",
    "react-dom": "^18.1.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "semantic-release": "^19.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://mkosir.github.io/demozap",
  "bugs": "https://github.com/mkosir/demozap/issues",
  "license": "MIT",
  "author": "Marko Kosir <kosir.mar@gmail.com>",
  "repository": "mkosir/demozap",
  "keywords": [
    "react",
    "react-component",
    "demo-component",
    "demo-generator",
    "demo",
    "demozap",
    "demo-cli",
    "demo-code",
    "cli",
    "cli-tool",
    "documentation",
    "documentation-tool",
    "oclif"
  ]
}
