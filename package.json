{
  "name": "story-tab",
  "description": "Generate documentation in Storybook tab",
  "version": "1.0.2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run postpack && npm run prepack",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint --max-warnings 0 --ext .js,jsx,ts,tsx src/",
    "lint-prettier": "prettier --config .prettierrc -l  src/**/*.ts{,x}",
    "prettier": "prettier --config .prettierrc --write src/**/*.ts{,x}",
    "validate": "npm run lint && npm run lint-prettier && npm run build",
    "test-storytab-react": "./bin/run generate react",
    "test-storytab-react-ts": "./bin/run generate react --ts"
  },
  "bin": {
    "storytab": "./bin/run"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "storytab",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.2",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^10.17.9",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "globby": "^10.0.1",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mkosir/story-tab",
  "bugs": "https://github.com/mkosir/story-tab/issues",
  "license": "MIT",
  "author": "Marko Kosir <kosir.mar@gmail.com>",
  "repository": "mkosir/story-tab",
  "keywords": [
    "oclif",
    "story-tab",
    "storytab",
    "storybook"
  ]
}
