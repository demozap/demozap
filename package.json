{
  "name": "react-doc-tab",
  "description": "Generate React component code as documentation tabs",
  "version": "1.0.22",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run postpack && npm run prepack",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint --max-warnings 0 --ext .js,jsx,ts,tsx src/",
    "lint-prettier": "prettier --config .prettierrc -l  src/**/*.ts{,x}",
    "prettier": "prettier --config .prettierrc --write {src,examples}/**/**/*",
    "validate": "npm run lint && npm run lint-prettier && npm run build",
    "storybook": "start-storybook -p 9009 -s public --quiet",
    "storybook:build": "build-storybook -c .storybook -o build_storybook --quiet",
    "generate-examples": "./bin/run generate --prefix=_"
  },
  "bin": {
    "doctab": "./bin/run"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "doctab",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.2",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "inquirer": "^7.0.1",
    "replace-in-file": "^5.0.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@storybook/react": "^5.2.8",
    "@types/node": "^10.17.9",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "globby": "^10.0.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react-doc-tab-template": "^1.0.12",
    "sass-loader": "^8.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "lint-staged": {
    "{src,examples}/**/**/*": [
      "prettier --config .prettierrc --write ",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mkosir/react-doc-tab",
  "bugs": "https://github.com/mkosir/react-doc-tab/issues",
  "license": "MIT",
  "author": "Marko Kosir <kosir.mar@gmail.com>",
  "repository": "mkosir/react-doc-tab",
  "keywords": [
    "react-doc-tab",
    "doc-tab",
    "doctab",
    "cli",
    "cli-tool",
    "documentation",
    "documentation-tool",
    "oclif"
  ]
}
