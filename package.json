{
  "name": "react-demo-tab-cli",
  "description": "CLI tool for creating demos of react components",
  "version": "1.2.50",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run postpack && npm run prepack",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint --max-warnings 0 --ext .js,jsx,ts,tsx src/",
    "lint-prettier": "prettier --config .prettierrc -l  src/**/*.ts{,x}",
    "prettier": "prettier --config .prettierrc --write {src,examples}/**/**/*",
    "validate": "npm run lint && npm run lint-prettier && npm run build",
    "storybook": "start-storybook -p 9009 -s public --quiet",
    "storybook:build": "build-storybook -c .storybook -o build_storybook --quiet",
    "demotab-examples": "./bin/run generate --prefix=_ && npm run prettier"
  },
  "bin": {
    "demotab": "./bin/run"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "demotab",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "chalk": "^4.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "inquirer": "^7.3.2",
    "replace-in-file": "^6.1.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@storybook/react": "^5.3.19",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.4.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "globby": "^11.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-demo-tab": "^1.2.18",
    "sass-loader": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "lint-staged": {
    "{src,examples}/**/**/*": [
      "prettier --config .prettierrc --write ",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mkosir/react-demo-tab-cli",
  "bugs": "https://github.com/mkosir/react-demo-tab-cli/issues",
  "license": "MIT",
  "author": "Marko Kosir <kosir.mar@gmail.com>",
  "repository": "mkosir/react-demo-tab-cli",
  "keywords": [
    "react",
    "react-component",
    "demo-component",
    "demo",
    "react-demo-tab-cli",
    "demo-tab-cli",
    "demotab",
    "demo-tab",
    "cli",
    "cli-tool",
    "documentation",
    "documentation-tool",
    "oclif",
    "code-generator",
    "code-example"
  ]
}
