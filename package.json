{
  "name": "react-demo-tab-cli",
  "description": "CLI tool for creating demos of react components",
  "version": "1.2.47",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run postpack && npm run prepack",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint --max-warnings 0 --ext .js,jsx,ts,tsx src/",
    "lint-prettier": "prettier --config .prettierrc -l  src/**/*.ts{,x}",
    "prettier": "prettier --config .prettierrc --write {src,examples}/**/**/*",
    "validate": "npm run lint && npm run lint-prettier && npm run build",
    "storybook": "start-storybook -p 9009 -s public --quiet",
    "storybook:build": "build-storybook -c .storybook -o build_storybook --quiet",
    "generate-examples": "./bin/run generate --prefix=_ && npm run prettier"
  },
  "bin": {
    "demotab": "./bin/run"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "demotab",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "inquirer": "^7.0.4",
    "replace-in-file": "^5.0.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@storybook/react": "^5.3.13",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "globby": "^11.0.0",
    "husky": "^4.2.2",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react-demo-tab": "^1.2.13",
    "sass-loader": "^8.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "lint-staged": {
    "{src,examples}/**/**/*": [
      "prettier --config .prettierrc --write ",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mkosir/react-demo-tab-cli",
  "bugs": "https://github.com/mkosir/react-demo-tab-cli/issues",
  "license": "MIT",
  "author": "Marko Kosir <kosir.mar@gmail.com>",
  "repository": "mkosir/react-demo-tab-cli",
  "keywords": [
    "react-demo-tab-cli",
    "demo-tab-cli",
    "demotab",
    "demo-tab",
    "cli",
    "cli-tool",
    "documentation",
    "documentation-tool",
    "oclif",
    "code-generator",
    "code-example"
  ]
}
